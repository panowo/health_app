<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.FourFragment.MyFragment">

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:layout_marginTop="236dp"
        android:background="@android:color/black"
        app:layout_constraintTop_toBottomOf="@+id/textView_id"
        tools:layout_editor_absoluteX="0dp" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:layout_marginTop="124dp"
        android:background="@android:color/black"
        app:layout_constraintTop_toBottomOf="@+id/textView_id"
        tools:layout_editor_absoluteX="2dp" />

    <TextView
        android:id="@+id/textView_id"
        android:layout_width="413dp"
        android:layout_height="36dp"
        android:layout_marginTop="4dp"
        android:text="账号:"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:layout_marginTop="8dp"
        android:background="@android:color/black"
        app:layout_constraintTop_toBottomOf="@+id/textView_id"
        tools:layout_editor_absoluteX="0dp" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:layout_marginTop="8dp"
        android:background="@android:color/black"
        app:layout_constraintTop_toBottomOf="@+id/textView_id"
        tools:layout_editor_absoluteX="0dp" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:layout_marginTop="180dp"
        android:background="@android:color/black"
        app:layout_constraintTop_toBottomOf="@+id/textView_id"
        tools:layout_editor_absoluteX="2dp" />

    <Button
        android:id="@+id/logout"
        android:layout_width="159dp"
        android:layout_height="60dp"
        android:background="@drawable/bin_logout"
        android:text="退出登录"
        android:textSize="25sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.888"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.815" />

    <Button
        android:id="@+id/btn_edit"
        android:layout_width="110dp"
        android:layout_height="54dp"
        android:layout_marginEnd="152dp"
        android:layout_marginBottom="24dp"
        android:text="编辑资料"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/logout"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/text_my"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:textAlignment="center"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/btn_timeupdate"
        android:layout_width="110dp"
        android:layout_height="54dp"
        android:layout_marginEnd="28dp"
        android:layout_marginBottom="24dp"
        android:text="定时上传"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/logout"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/textView_name"
        android:layout_width="413dp"
        android:layout_height="36dp"
        android:layout_marginTop="20dp"
        android:text="昵称:"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView_id" />

    <ImageView
        android:id="@+id/imageView1"
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:layout_marginTop="64dp"
        android:background="@android:color/black"
        app:layout_constraintTop_toBottomOf="@+id/textView_id"
        tools:layout_editor_absoluteX="0dp" />

    <TextView
        android:id="@+id/textView_gender"
        android:layout_width="413dp"
        android:layout_height="36dp"
        android:layout_marginTop="20dp"
        android:text="性别:"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView_name" />

    <TextView
        android:id="@+id/textView_age"
        android:layout_width="413dp"
        android:layout_height="36dp"
        android:layout_marginTop="20dp"
        android:text="年龄:"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView_gender" />

    <TextView
        android:id="@+id/textView_height"
        android:layout_width="413dp"
        android:layout_height="36dp"
        android:layout_marginTop="20dp"
        android:text="身高:"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView_age" />

    <TextView
        android:id="@+id/textView_weight"
        android:layout_width="413dp"
        android:layout_height="36dp"
        android:layout_marginTop="20dp"
        android:text="体重:"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView_height" />


</androidx.constraintlayout.widget.ConstraintLayout>