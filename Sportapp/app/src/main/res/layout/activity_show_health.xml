<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".pages.ShowHealthActivity">

    <TextView
        android:id="@+id/health_state2"
        android:layout_width="57dp"
        android:layout_height="29dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="48dp"
        android:layout_weight="1"
        android:gravity="center"
        android:text="状态"
        android:textSize="15dp"
        app:layout_constraintStart_toEndOf="@+id/health_tem2"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/health_tem2"
        android:layout_width="47dp"
        android:layout_height="31dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="48dp"
        android:layout_weight="1"
        android:gravity="center"
        android:text="体温"
        android:textSize="15dp"
        app:layout_constraintStart_toEndOf="@+id/health_place2"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/health_place2"
        android:layout_width="113dp"
        android:layout_height="30dp"
        android:layout_marginStart="22dp"
        android:layout_marginTop="50dp"
        android:layout_weight="1"
        android:gravity="center"
        android:text="地点"
        android:textSize="15dp"
        app:layout_constraintStart_toEndOf="@+id/health_time2"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/health_time2"
        android:layout_width="0dp"
        android:layout_height="30dp"
        android:layout_marginStart="68dp"
        android:layout_marginTop="48dp"
        android:layout_weight="1"
        android:gravity="center"
        android:text="时间"
        android:textSize="15dp"
        app:layout_constraintStart_toEndOf="@+id/health_date2"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/health_date2"
        android:layout_width="0dp"
        android:layout_height="30dp"
        android:layout_marginTop="48dp"
        android:layout_weight="1"
        android:gravity="center"
        android:text="日期"
        android:textSize="15dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ListView
        android:id="@+id/list_health"
        android:layout_width="406dp"
        android:layout_height="536dp"
        app:layout_constraintTop_toBottomOf="@+id/health_tem2"
        tools:layout_editor_absoluteX="2dp" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="健康打卡记录"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>